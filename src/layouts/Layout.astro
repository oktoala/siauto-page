---
export interface Props {
  title: string;
}
import logo from "../icons/logo.svg";
import ogImg from "../icons/og-show.png";

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href={logo} />
    <meta name="theme-color" content="#fff" />
    <meta name="description" content="Untuk Kalian Yang Malas Isi Kuesioner" />
    <link rel="apple-touch-icon" type="image/svg+xml" href={logo} />
    <meta name="generator" content={Astro.generator} />
    <meta property="og:title" content="SIAuto" />
    <meta property="og:type" content="website" />
    <meta property="og:image" content={ogImg} />
    <meta property="og:url" content="https://siauto.dalamkota.com/" />
    <meta
      property="og:description"
      content="Untuk Kalian Yang Malas Isi Kuesioner KHS di sia.unmul.ac.id"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css"
      integrity="sha512-Fo3rlrZj/k7ujTnHg4CGR2D7kSs0v4LLanw2qksYuRlEzO+tcaEPQogQ0KaoGN26/zrn20ImR1DfuLWnOo7aBA=="
      crossorigin="anonymous"
    />
    <title>{title}</title>
  </head>
  <body class="overflow-x-hidden">
    <slot />
  </body>
</html>
